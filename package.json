{
  "name": "kikuldetesek",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@nutrient-sdk/viewer": "^1.0.0",
    "@sap/cds": "^8",
    "@sap/cds-common-content": "^2.1.0",
    "@sap/xssec": "^4.4.0",
    "blob-stream": "^0.1.3",
    "cloudmersive-convert-api-client": "^2.8.0",
    "exceljs": "^4.4.0",
    "express": "^4",
    "pdfkit": "^0.16.0",
    "pdfkit-table": "^0.1.99",
    "pdfmake": "^0.2.18",
    "puppeteer": "^24.3.0",
    "soap": "^1.1.10",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.8.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy kikuldetesek --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-mainview": "cds watch --open mainview/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-postingregularui": "cds watch --open postingregularui/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-postingwithcarui": "cds watch --open postingwithcarui/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-listview": "cds watch --open listview/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-adminpanel": "cds watch --open adminpanel/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "auth": "xsuaa",
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db/kikuldetesek.sqlite"
        }
      }
    }
  },
  "sapux": [
    "app/postingwithcarui",
    "app/adminpanel"
  ]
}
